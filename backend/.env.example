PTERODACTYL_BASE_URL=https://panel.example.com
PTERODACTYL_API_KEY=ptla_your_application_api_key
PTERODACTYL_NEST_ID=1
PTERODACTYL_EGG_ID=1
PTERODACTYL_DEFAULT_NODE_ID=1
PTERODACTYL_DEFAULT_LOCATION_ID=1
PTERODACTYL_DEFAULT_ALLOCATION_ID=1
PTERODACTYL_DEFAULT_DOCKER_IMAGE=ghcr.io/pterodactyl/yolks:java_17
PTERODACTYL_DEFAULT_STARTUP=java -Xms128M -Xmx{{SERVER_MEMORY}}M -jar server.jar
